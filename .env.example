# Node environment
NODE_ENV=development

# Redis URL (optional for development, uses in-memory queue)
REDIS_URL=

# File limits
MAX_FILE_SIZE=104857600

# Storage paths
UPLOAD_PATH=./uploads
OUTPUT_PATH=./processed

# YouTube download options
YOUTUBE_QUALITY=highestaudio

# Processing options
ENABLE_CLEANUP=true
CLEANUP_INTERVAL=3600000